COLUMN TABLE "DIM_PERIODO_VERSION"(
	CD_PERIODO VARCHAR(7)  NOT NULL,
	CD_VERSION smallint,
	FL_ESTADO VARCHAR(1),
	FL_NOTAS VARCHAR(1),
	FC_DESDE DATE,
	FC_HASTA DATE,
	FECHA_HORA TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
	USUARIO NVARCHAR(50) DEFAULT 'HANA',
	PRIMARY KEY (
		"CD_PERIODO", CD_VERSION
	)
)
UNLOAD PRIORITY 5 AUTO MERGE;