COLUMN TABLE "DIM_TASAS_MONEDAS"(
	CD_FMONEDA VARCHAR(4) NOT NULL,
	CD_TMONEDA VARCHAR(4) NOT NULL,
	FC_FECHA DATE,
	CD_TIPO_DIA VARCHAR(1),
	VL_TASA DECIMAL(17, 7),
	FECHA_HORA TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
	USUARIO NVARCHAR(50) DEFAULT 'HANA',
	PRIMARY KEY (
		"CD_FMONEDA",
		"CD_TMONEDA",
		"FC_FECHA"
	)
)
UNLOAD PRIORITY 5 AUTO MERGE;