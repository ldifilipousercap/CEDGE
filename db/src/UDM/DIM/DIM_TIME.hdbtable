COLUMN TABLE "DIM_TIME"(
    "YEAR" NVARCHAR(4) NOT NULL,
    "HALFYEAR" NVARCHAR(2),
    "QUARTER" NVARCHAR(2),
    "MONTH" NVARCHAR(2) NOT NULL ,
    "CALQUARTER" NVARCHAR(5),
    "CALMONTH" NVARCHAR(6),
    "YEAR_INT" INTEGER,
    "HALFYEAR_INT" INTEGER,
    "QUARTER_INT" INTEGER,
    "MONTH_INT" INTEGER ,
    "CD_PERIODO" VARCHAR(7),
    "NM_PERIODO" VARCHAR(3),
PRIMARY KEY ("YEAR","MONTH")) 
UNLOAD PRIORITY 5 AUTO MERGE;