COLUMN TABLE "DIM_TASAS_BPC"(
	CD_MONEDA VARCHAR(4) NOT NULL,
	FC_FECHA DATE,
	CD_TIPO_TASA VARCHAR(5) NOT NULL,
	CD_VERSION SMALLINT NOT NULL,
	VL_TASA DECIMAL(17, 7),
	FECHA_HORA TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
	USUARIO NVARCHAR(50) DEFAULT 'HANA',
	PRIMARY KEY (
		"CD_MONEDA",
		"FC_FECHA",
		"CD_TIPO_TASA",
		"CD_VERSION"
	)
)
UNLOAD PRIORITY 5 AUTO MERGE;